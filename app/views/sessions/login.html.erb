<h1>Log In Here</h1>

<p><%= flash[:error] %></p> 

<%= form_for(:user) do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Log In" %>
    

<% end %> 

<a href="/">Back</a>


